@using WebApplication1.Models
@model portalcodes

@{
    ViewBag.Title = "getdecuments_list";
    Layout = "~/Views/Shared/_layout.cshtml";
    //List<service> services = ViewBag.servicelistview;
    List<Resultbill> rest_bill_list = ViewBag.rest_bill_list;
}

<div class="col-lg-12 grid-margin stretch-card" dir="rtl">
    <div class="card" dir="rtl">
        <div class="card-body" dir="rtl">
            <h4 class="card-title">فواتير سابقة</h4>
          
          
            <div class="form-group"></div>
            <table class="table table-dark">
                <thead>
                    <tr>

                        <th> الرقم الداخلى </th>

                        <th>المرسل </th>

                        <th>المستلم </th>


                        <th>التاريخ الإستلام</th>
                        <th> الإجمالى</th>
                        <th>الحالة</th>
                        <th></th>
                       
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in rest_bill_list)
                    {
                    <tr>

                        <td>@item.internalId</td>
                        <td>@item.issuerName </td>
                        <td>@item.receiverName </td>
                        <td>@item.dateTimeReceived.ToShortDateString()</td>
                        <td>@item.total</td>
                        <td>@item.status</td>
                       
                        <td><a href="/get_decuments/print_out/@item.uuid"> طباعة</a></td>
                        


                    </tr>
                    }
                </tbody>
            </table>
            <h2>@ViewBag.token</h2>
            <h2> @ViewBag.stauscode</h2>
            <h2>@ViewBag.tokendata</h2>
        </div>
    </div>
</div>
