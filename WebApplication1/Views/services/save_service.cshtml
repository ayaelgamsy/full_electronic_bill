@using WebApplication1.Models
@model portalcodes
@{
    Layout = "~/Views/Shared/_layout.cshtml";
    //List<service> services = ViewBag.servicelistview;
    List<Result> rescodeslist = ViewBag.rescodeslist;

}

<div class="col-lg-12 grid-margin stretch-card" dir="rtl">
    <div class="card" dir="rtl">
        <div class="card-body" dir="rtl">
            <h4 class="card-title">بيانات الخدمات</h4>
            <div class="form-group"></div>
            <div class="template-demo">
                <button type="submit" class="btn btn-success btn-fw" onclick="window.location.href='/services/services'">إضافة خدمة </button>

            </div>
            <div class="form-group"></div>
            <table class="table table-dark">
                <thead>
                    <tr>

                        <th> النوع </th>

                        <th>الرقم المميز </th>

                        <th> اسم التصنيف </th>


                        <th>الوصف</th>
                        <th> نشط من</th>
                        <th> نشط إلى</th>
                        <th> نشط</th>
                        <th> الحالة</th>
                    </tr>
                </thead>
                <tbody>
            @foreach (var item in rescodeslist)
            {
            <tr>

                <td>@item.codeTypeName</td>
                <td>@item.itemCode </td>
                <td>@item.codeNamePrimaryLang </td>
                <td>@item.descriptionPrimaryLang</td>
                <td>@item.activeFrom</td>
                <td>@item.activeTo</td>
                <td>@item.active</td>
                <td>@item.status</td>

            </tr>
            }
        </tbody>
            </table>
            <h2>@ViewBag.token</h2>
            <h2> @ViewBag.stauscode</h2>
            <h2>@ViewBag.tokendata</h2>
        </div>
    </div>
</div>